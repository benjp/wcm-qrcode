<% groups.each { group -> %>
  <div class="group">
  <div>${group.groupName}</div>
  <%
    def a = memberships[group.id];
    if (a != null)
    {
      %>
        <ul>
          <% a.each { membership -> %>
            <li>${membership}</li>
          <% } %>
        </ul>
      <%
    }
  %>
  </div>
<% } %>
